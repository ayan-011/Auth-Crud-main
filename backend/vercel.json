{
    "installerCommand":" npm install --legacy-peer-deps",
    "verson": 2,
    "builds":[
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }  
    ],
    "routes":[
        {"src": "/(.*)","dest": "/"}
    ]
}